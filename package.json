  "license": "Apache-2.0",
  "dependencies": {
    "@blendededge/ferryman-extensions": "^2.0.6",
    "@elastic.io/oih-standard-library": "^2.0.3",
    "@openintegrationhub/ferryman": "^2.3.1",
    "async": "^3.2.4",
    "micromatch": "^4.0.5",
    "moment": "^2.29.4",
    "ssh2-sftp-client": "^9.1.0"
  },
  "devDependencies": {
    "bunyan": "^1.8.15",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "mocha": "^10.2.0",
    "nock": "^13.3.3",
    "sinon": "^16.0.0"
